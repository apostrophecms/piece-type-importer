{
  "extends": [ "apostrophe", "plugin:vue/vue3-recommended" ],
  "rules": {
    "no-var": "error",
    "no-unused-vars": [
      "error",
      { "varsIgnorePattern": "^_.", "args": "none" }
    ]
  },
  "globals": {
    "apos": true
  },
  "overrides": [
    {
      "files": [
        "**/ui/**/*.js"
      ],
      "globals": {
        "apos": true
      }
    }
  ]
}
